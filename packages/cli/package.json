    "lint": "eslint --color src spec",
    "flow-typed": "flow-typed update",
    "prepublish": "npm run compile"
  },
  "bin": {
    "dicy": "lib/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yitzchak/dicy.git"
  },
  "author": "Tarn Burton <twburton@gmail.com>",
