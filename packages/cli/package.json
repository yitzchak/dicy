{
  "name": "@dicy/cli",
  "version": "0.11.0",
  "description": "Command line interface to DiCy, a builder for LaTeX, knitr, literate Agda, literate Haskell and Pweave that automatically builds dependencies.",
  "keywords": [
    "latex",
    "knitr",
    "sage",
    "biblatex"
  ],
  "bugs": "https://github.com/yitzchak/dicy/issues",
  "main": "lib/main.js",
  "scripts": {
    "test": "jasmine",
    "compile": "rimraf lib && tsc",
    "prepublish": "npm run compile"
  },
  "bin": {
    "dicy": "lib/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yitzchak/dicy.git"
  },
  "author": "Tarn Burton <twburton@gmail.com>",
  "contributors": [],
  "homepage": "https://yitzchak.github.io/dicy/",
  "engines": {
    "node": ">=6.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@dicy/core": "^0.11.0",
    "chalk": "^2.3.0",
    "cliui": "^3.2.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.9.1",
    "@types/lodash": "^4.14.80",
    "@types/yargs": "^8.0.2",
    "rimraf": "^2.6.1",
    "typescript": "^2.5.3"
  },
  "yargs": {
    "dot-notation": false,
    "boolean-negation": false,
    "camel-case-expansion": false
  }
}
