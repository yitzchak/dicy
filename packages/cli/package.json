{
  "name": "@dicy/cli",
  "version": "0.13.0",
  "description": "Command line interface to DiCy, a builder for LaTeX, knitr, literate Agda, literate Haskell and Pweave that automatically builds dependencies.",
  "keywords": [
    "latex",
    "knitr",
    "sage",
    "biblatex"
  ],
  "bugs": "https://github.com/yitzchak/dicy/issues",
  "main": "lib/main.js",
  "scripts": {
    "test": "jasmine-ts",
    "compile": "rimraf lib && tsc && prepend lib/main.js \"#!/usr/bin/env node\n\"",
    "prepublish": "npm run compile"
  },
  "bin": {
    "dicy": "lib/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yitzchak/dicy.git"
  },
  "author": "Tarn Burton <twburton@gmail.com>",
  "contributors": [],
  "homepage": "https://yitzchak.github.io/dicy/",
  "engines": {
    "node": ">=6.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@dicy/core": "^0.13.0",
    "chalk": "^2.4.1",
    "cliui": "^4.1.0",
    "file-uri-to-path": "^1.0.0",
    "file-url": "^2.0.2",
    "fs-extra": "^7.0.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "string-width": "^2.1.1",
    "wrap-ansi": "^4.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/wrap-ansi": "^2.0.14",
    "@types/yargs": "^11.0.0",
    "jasmine": "^3.2.0",
    "jasmine-ts": "^0.2.1",
    "prepend-file-cli": "^1.0.6",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  },
  "yargs": {
    "dot-notation": false,
    "boolean-negation": false,
    "camel-case-expansion": false
  }
}
